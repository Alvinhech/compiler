const int n = 5;
int i,j,a[5],temp;
void main()
{
printf("Please enter 5 numbers:");
for ( i=0; i<n; i=i+1)
{
scanf(temp);
a[i]=temp;
}
for ( i=0; i<n; i=i+1)
for ( j=i+1; j<n; j=j+1)
{
if(a[i]>a[j]){
temp = a[i];
a[i] = a[j];
a[j] = temp;
}
}
printf("SortedArray:");
for ( i=0; i<n; i=i+1)
printf(" ",a[i]);
return;
}
